import java.util.Scanner;

public class ej4 {
    public static void main (String []args){
        int aux=1 ;
        Scanner in=new Scanner(System.in);
        System.out.println("Digite un numero: ");
        int n=in.nextInt();
        for (int i =2 ;i<=n; i++){
            aux*=i;
        } System.out.println("El factorial de "+n+" es "+aux);


    }
}
