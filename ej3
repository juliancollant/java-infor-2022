import java.util.Scanner;

public class ej3 {
    public static void main (String[]args){
        Scanner in = new Scanner(System.in);
        System.out.println("Digite un numero : ");
        int n=in.nextInt();
        for (int i=1; i<=n ; i++){
            System.out.println(" ");
            for (int j=1; j<=i; j++){
                System.out.print(j);
            }

        }
        System.out.println(" ");
        }
    }

